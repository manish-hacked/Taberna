<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:weightSum="100"
    android:background="@drawable/homepage"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="70"
        android:id="@+id/top"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="30">
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1"
            android:id="@+id/p"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:background="@android:drawable/dialog_holo_light_frame">
                <ImageButton
                    android:id="@+id/action_voice_btn"
                    style="@style/MSV_ImageButton"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_action_action_search"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <EditText android:id="@+id/username"
                    android:layout_width="match_parent"
                    android:layout_height="59dp"
                    android:hint="Search"
                    android:inputType="text"
                    android:background="@android:color/transparent"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="1"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/p"
            android:orientation="vertical">
            <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/table_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:id="@+id/row_12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_bt7"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/pooks"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Books"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/textView456" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"

                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_t7"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/toys"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Toys"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/textView5" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_tn"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/cloths"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Cloths"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/textView45" />
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:id="@+id/row_12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_bts7"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/posmetic"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Cosmetics"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/textView456" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_tdeq7"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/pofa"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Furnitures"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/tedxtView5" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:layout_weight="1"
                        android:layout_margin="0dp"
                        android:padding="0dp"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:textColor="#fff"
                        android:weightSum="1">
                        <ImageButton
                            android:id="@+id/action_voice_tdn"
                            style="@style/MSV_ImageButton"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:src="@drawable/qlec"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_weight="1.28" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:textAppearance="?android:attr/textAppearanceSmall"
                            android:text="Electronics"
                            android:gravity="center"
                            android:textSize="20sp"
                            android:textColor="#FFFFFF"
                            android:id="@+id/textView4d5" />
                    </LinearLayout>
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
